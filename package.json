{
  "name": "filmstrip-web",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/yenbekbay/filmstrip-web",
  "repository": "yenbekbay/filmstrip-web",
  "license": "GPL-3.0",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "deploy": "sh ./scripts/deploy.sh",
    "start": "next start",
    "build": "next build",
    "dev": "next",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg",
    "sync-graphql-schema": "babel-node scripts/fetch-graphql-schema.js",
    "generate-tachyons": "babel-node scripts/generate-tachyons.js",
    "test": "npm-run-all --parallel typecheck lint",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "flow check ."
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!.next/**",
      "!flow-typed/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "jsdom",
    "testPathDirs": [
      "<rootDir>/components",
      "<rootDir>/data",
      "<rootDir>/hocs",
      "<rootDir>/pages"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "apollo-client": "^0.8.1",
    "counterpart": "^0.17.6",
    "dont-go": "^1.0.7",
    "glob": "^7.1.1",
    "graphql-tag": "^1.2.4",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.17.4",
    "next": "^1.2.3",
    "react": "^15.4.2",
    "react-apollo": "^0.9.0",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-image-lightbox": "^3.4.2",
    "react-interpolate-component": "^0.10.0",
    "react-modal": "^1.6.5",
    "react-progressive-image": "^0.1.2",
    "react-redux": "^5.0.2",
    "react-select": "^1.0.0-rc.2",
    "react-translate-component": "^0.13.2",
    "react-youtube": "^7.3.0",
    "redux": "^3.6.0",
    "redux-persist": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "css": "^2.2.1",
    "eslint": "^3.13.1",
    "eslint-config-anvilabs": "^4.0.0",
    "eslint-plugin-graphql": "^0.6.0",
    "flow-bin": "^0.38.0",
    "graphql": "^0.9.1",
    "husky": "^0.13.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^18.1.0",
    "npm-run-all": "^4.0.1",
    "validate-commit-msg": "^2.11.1"
  }
}
