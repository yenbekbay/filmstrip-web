{
  "name": "filmstrip-web",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/yenbekbay/filmstrip-web",
  "repository": "yenbekbay/filmstrip-web",
  "license": "GPL-3.0",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "typecheck": "flow check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "npm-run-all --parallel typecheck lint",
    "sync-graphql-schema": "babel-node scripts/fetch-graphql-schema.js",
    "generate-tachyons": "babel-node scripts/generate-tachyons.js",
    "start": "nodemon src/server.js",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test",
    "clean": "rimraf build && mkdir build",
    "build": "npm run clean && babel --out-dir build --ignore **/__tests__,**/__mocks__ src && next build app && sh scripts/fix-require-paths.sh",
    "deploy": "sh scripts/deploy.sh"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/*.js",
      "!app/.next/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "jsdom",
    "testPathDirs": [
      "<rootDir>/app"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "lint-staged": {
    "{app,scripts,src}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "apollo-client": "^0.8.7",
    "babel-register": "^6.23.0",
    "counterpart": "^0.17.6",
    "dont-go": "^1.0.7",
    "express": "^4.14.1",
    "glamor": "^2.20.23",
    "graphql-tag": "^1.2.4",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.17.4",
    "next": "^2.0.0-beta.31",
    "react": "^15.4.2",
    "react-apollo": "^0.11.1",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-image-lightbox": "^3.4.2",
    "react-interpolate-component": "^0.10.0",
    "react-modal": "^1.6.5",
    "react-progressive-image": "^0.1.2",
    "react-redux": "^5.0.2",
    "react-select": "^1.0.0-rc.3",
    "react-translate-component": "^0.13.2",
    "react-youtube": "^7.3.0",
    "redux": "^3.6.0",
    "redux-persist": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css": "^2.2.1",
    "eslint": "^3.15.0",
    "eslint-config-anvilabs": "^5.0.0",
    "eslint-plugin-graphql": "^0.6.0",
    "flow-bin": "^0.38.0",
    "graphql": "^0.9.1",
    "husky": "^0.13.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^18.1.0",
    "lint-staged": "^3.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "prettier": "^0.18.0",
    "rimraf": "^2.5.4",
    "validate-commit-msg": "^2.11.1"
  }
}
